    .label = नक़ल लिअ
    .accesskey = C
config-copy-name =
    .label = नाम क नक़ल लिअ
    .accesskey = N
config-copy-value =
    .label = मान क नक़ल लिअ
    .accesskey = V
config-modify =
    .label = बदलू
    .accesskey = M
config-toggle =
    .label = टागल  करू
    .accesskey = T
config-reset =
    .label = फेर सेट  करू
