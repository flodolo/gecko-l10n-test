config-window =
    .title = about:config

## Strings used to display a warning in about:config

# This text should be attention grabbing and playful
config-about-warning-title =
    .value = This might void your warranty!
config-about-warning-text = Makadaddael dagitoy a sagana ti nangatngato a tukad iti simpa, seguridad ken pannakaaramid daytoy nga aplikasion. Agpatuloykayo koma no siguradokayo kadagiti aramidenyo.
config-about-warning-checkbox =
    .label = Show this warning next time

config-search-prefs =
    .value = Search:
    .accesskey = r

config-focus-search =
    .key = P

## These strings are used for column headers

config-pref-column =
    .label = Nagan ti Pagdumdumaan
config-lock-column =
    .label = Kasasaad
config-type-column =
    .label = Kita
config-value-column =
    .label = Pateg

## These strings are used for tooltips

config-pref-column-header =
    .tooltip = Klikan tapno matakunda
config-column-chooser =
    .tooltip = Click to select columns to display

## These strings are used for the context menu

config-copy-pref =
    .key = C
    .label = Kopiaen
    .accesskey = C

config-copy-name =
    .label = Kopiaen ti Nagan
    .accesskey = N

config-copy-value =
    .label = Kopiaen ti Pateg
    .accesskey = V

config-modify =
    .label = Baliwan
    .accesskey = M

config-toggle =
    .label = Agkasukat
    .accesskey = t

config-reset =
    .label = Reset
    .accesskey = R

config-new =
    .label = Baro
    .accesskey = w

config-string =
    .label = Kawar
    .accesskey = S

config-integer =
    .label = Dagup
    .accesskey = I

config-boolean =
    .label = Lohiko
    .accesskey = B

config-default = default
config-locked = natulbekan

config-property-string = kawar
config-property-bool = lohiko

config-new-prompt = Isurat ti nagan ti pagdumdumaan

config-nan-title = Awan pateg

# Variables:
#   $type (String): type of value (boolean, integer or string)
config-new-title = Baro a pateg ti { $type }

# Variables:
#   $type (String): type of value (boolean, integer or string)
config-modify-title = Isurat ti pateg a { $type }
